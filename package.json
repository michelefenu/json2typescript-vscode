{
  "name": "json2typescript",
  "displayName": "JSON 2 TypeScript",
  "private": true,
  "description": "Convert JSON data to TypeScript interfaces",
  "version": "0.0.2",
  "publisher": "michelefenu",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Other"
  ],
  "preview": true,
  "activationEvents": [
    "onCommand:json2typescript.generate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json2typescript.generate",
        "title": "Generate TypeScript interfaces from JSON clipboard"
      }
    ],
    "keybindings": [
      {
        "command": "json2typescript.generate",
        "key": "alt+shift+V"
      }
    ]
  },
  "icon": "https://raw.githubusercontent.com/michelefenu/json2typescript-vscode/master/assets/icon256x256.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@semantic-release/github": "^7.1.1",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "json2interface": "^0.1.9"
  },
  "release": {
		"branches": "master",
		"verifyConditions": [
			"@semantic-release/github"
		],
		"publish": [
			"@semantic-release/github"
		],
		"success": [
			"@semantic-release/github"
		],
		"fail": [
			"@semantic-release/github"
		]
	}
}
